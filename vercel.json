{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.italky.ai" }],
      "destination": "https://italky.ai/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/pages/:path*", "destination": "/pages/:path*" },
    { "source": "/js/:path*", "destination": "/js/:path*" },
    { "source": "/css/:path*", "destination": "/css/:path*" },
    { "source": "/assets/:path*", "destination": "/assets/:path*" },
    { "source": "/icons/:path*", "destination": "/icons/:path*" },
    { "source": "/fonts/:path*", "destination": "/fonts/:path*" },
    { "source": "/favicon.ico", "destination": "/favicon.ico" }
  ]
}
